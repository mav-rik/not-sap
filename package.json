{
  "name": "notsap",
  "version": "0.0.4",
  "description": "Monorepo for @notsap packages",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "oxlint -c .oxlintrc.json",
    "build": "pnpm -r before-build && node ./scripts/build.js",
    "build-only": "node ./scripts/build.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "version": "pnpm changelog && git add CHANGELOG.md && git commit -m 'chore: update changelog'",
    "release": "pnpm build && pnpm test && node ./scripts/versions && pnpm -r run pub",
    "vscode": "node ./scripts/vscode-publish",
    "sync": "node ./scripts/versions"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@prostojs/dye": "^0.4.9",
    "@types/node": "^22.5.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "conventional-changelog-cli": "^5.0.0",
    "glob": "^11.0.3",
    "husky": "^9.1.7",
    "inquirer": "^12.3.2",
    "oxlint": "^0.15.6",
    "prettier": "^3.3.3",
    "rolldown": "1.0.0-beta.1",
    "rollup": "^4.30.1",
    "rollup-plugin-dts": "^6.1.1",
    "tslib": "^2.6.3",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
    "unocss": "^66.5.2",
    "unplugin-swc": "^1.5.1",
    "vite": "^5.4.2",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "3.2.4",
    "zx": "^8.3.0"
  },
  "packageManager": "pnpm@9.7.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  }
}

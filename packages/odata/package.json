{
  "name": "notsapodata",
  "version": "0.0.3",
  "description": "OData utilities and client package",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "types": "./dist/vite/index.d.ts",
      "import": "./dist/vite/index.mjs",
      "require": "./dist/vite/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "pub": "pnpm publish --access public",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "exceljs": "^4.4.0",
    "fast-xml-parser": "^4.4.1"
  },
  "devDependencies": {
    "@types/node": "^22.5.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.2",
    "vitest": "^2.0.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mav-rik/not-sap.git",
    "directory": "packages/odata"
  },
  "bugs": {
    "url": "https://github.com/mav-rik/not-sap/issues"
  },
  "homepage": "https://github.com/mav-rik/not-sap/tree/main/packages/odata#readme",
  "build": [
    {
      "entries": [
        "src/index.ts"
      ],
      "dts": true
    },
    {
      "entries": [
        "src/vite/index.ts"
      ],
      "dts": true
    }
  ]
}
